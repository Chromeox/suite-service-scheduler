# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# QR Code API (if needed)
VITE_QR_CODE_API_URL=https://api.qrserver.com/v1/create-qr-code/

# Security Configuration
VITE_ENCRYPTION_KEY=your_random_32_character_string
VITE_CSRF_SECRET=your_random_32_character_string
VITE_APP_URL=https://your-app-url.com
VITE_ALLOWED_ORIGINS=https://your-app-url.com,https://admin.your-app-url.com

# Content Security Policy
VITE_CSP_REPORT_URI=https://your-report-collector.com/csp-reports

# Rate Limiting
VITE_RATE_LIMIT_MAX_ATTEMPTS=5
VITE_RATE_LIMIT_WINDOW_MS=60000

# Other API Keys (add as needed)
# VITE_OTHER_API_KEY=your_other_api_key
